{
  "name": "cluster-express-overloaded",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run -s build",
    "forever": "npm run build && forever start --uid ceo --append dist/bin/www",
    "start": "node dist/bin/www --exec \"node -r babel-register\"",
    "start-dev": "nodemon bin/www --exec \"node -r babel-register\" -w .",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel ./src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "dependencies": {
    "async": "^2.4.0",
    "body-parser": "~1.17.1",
    "bunyan": "^1.8.10",
    "cacheman": "^2.2.1",
    "cacheman-file-cluster": "0.0.4",
    "cacheman-redis": "^1.1.2",
    "colors": "^1.1.2",
    "connect-redis": "^3.3.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-session": "^1.15.2",
    "got": "^6.7.1",
    "later": "^1.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "morgan": "~1.8.1",
    "node-sass-middleware": "0.9.8",
    "nodemailer": "^4.0.1",
    "pug": "^2.0.0-rc.1",
    "require-directory": "^2.1.1",
    "serve-favicon": "~2.4.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.9.0",
    "babel-jest": "^20.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^2.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^2.1.2",
    "gulp-watch": "^4.3.11",
    "jest": "^20.0.0",
    "nodemon": "^1.9.2",
    "pump": "^1.0.2",
    "regenerator-runtime": "^0.10.5"
  }
}
